> printAll <- function( x ) {
+    for( n in names( x ) ) {
+       cat( "$", n, "\n", sep = "" )
+       if( n %in% c( "olsParam", "gridAdj", "gridParam", "mleParam", "olsStdEr", 
+             "mleCov", "resid", "fitted", "olsResid" ) ) {
+          print( round( x[[ n ]], 2 ) )
+       } else {
+          print( x[[ n ]] )
+       }
+       cat( "\n" )
+    }
+    cat( "class\n" )
+    print( class( x ) )
+ }
> 
> printME <- function( x ) {
+    me <- attr( x, "margEff" )
+    attr( x, "margEff" ) <- NULL
+    print( round( x, 2 ) )
+    if( !is.null( me ) ) {
+       cat( "margEff\n" )
+       print( round( me, 2 ) )
+    }
+ }
