> printAll <- function( x ) {
+    for( n in names( x ) ) {
+       cat( "$", n, "\n", sep = "" )
+       if( n %in% c( "olsParam", "gridParam", "mleParam", "olsStdEr", "mleCov" ) ) {
+          print( round( x[[ n ]], 2 ) )
+       } else if( n %in% c( "resid", "olsResid" ) ) {
+          print( round( x[[ n ]], 3 ) )
+       } else {
+          print( x[[ n ]] )
+       }
+       cat( "\n" )
+    }
+    cat( "class\n" )
+    print( class( x ) )
